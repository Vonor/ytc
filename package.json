{
  "name": "yaml-template-converter",
  "bin": {
    "ytc": "dist/ytc.js"
  },
  "version": "0.0.2",
  "description": "Yaml templating for config files",
  "main": "dist/ytc.js",
  "scripts": {
    "start": "babel-node src/ytc.js",
    "build": "babel src --out-dir dist",
    "pkg": "npm run build && pkg -t node10-linux -o pkg/ytc ."
  },
  "keywords": [
    "yaml",
    "template",
    "config",
    "json",
    "cli"
  ],
  "author": "Michael Melcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vonor/ytc.git"
  },
  "license": "GPL-3.0-or-later",
  "pkg": {
    "scripts": "dist/**/*.js"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-async-to-generator",
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0",
    "@babel/parser": "^7.5.0",
    "@babel/runtime": "^7.5.4",
    "commander": "^2.20.0",
    "handlebars": "^4.7.7",
    "isvalid": "^2.4.6",
    "js-yaml": "^3.13.1",
    "mapTypes": "github:vonor/maptypes"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "pkg": "^4.4.0"
  }
}
